{
  "name": "@agenticmail/openclaw",
  "version": "0.5.0",
  "description": "OpenClaw plugin and skill for AgenticMail",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "scripts",
    "skill",
    "README.md",
    "REFERENCE.md",
    "LICENSE"
  ],
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "scripts": {
    "build": "tsup index.ts --format esm --dts --clean",
    "test": "vitest run --passWithNoTests",
    "preuninstall": "node scripts/uninstall.mjs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agenticmail/agenticmail.git",
    "directory": "packages/openclaw"
  },
  "homepage": "https://github.com/agenticmail/agenticmail",
  "bugs": "https://github.com/agenticmail/agenticmail/issues",
  "keywords": [
    "openclaw",
    "email",
    "ai",
    "ai-agent",
    "agenticmail",
    "plugin",
    "skill"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Ope Olatunji",
  "license": "MIT"
}
